{
    "drone_dimensions": [
        "device_type=none",
        "os=Mac-12",
        "cpu=arm64",
        "mac_model=Macmini8,1"
    ],
    "gclient_variables": {
        "download_android_deps": false
    },
    "gn": [
        "--runtime-mode",
        "debug",
        "--prebuilt-dart-sdk",
        "--no-lto",
        "--force-mac-arm64"
    ],
    "name": "host_debug",
    "ninja": {
        "config": "host_debug"
    },
    "tests": [
        {
            "language": "python3",
            "name": "test: lint host_debug",
            "parameters": [
                "--variant",
                "host_debug",
                "--lint-all",
                "--shard-id=1",
                "--shard-variants=host_debug"
            ],
            "script": "flutter/ci/lint.sh"
        }
    ]
}
