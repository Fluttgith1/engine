{
    children: [
        {
            name: "flutter_jit_runner",
            url: "fuchsia-pkg://fuchsia.com/flutter_jit_runner#meta/flutter_jit_runner.cm",
        },
        {
            name: "two_flutter",
            url: "#meta/two-flutter.cm",
            environment: "#two_flutter_env",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.logger.LogSink",
                "fuchsia.sysmem.Allocator",
                "fuchsia.tracing.provider.Registry",
                "fuchsia.ui.scenic.Scenic",
                "fuchsia.vulkan.loader.Loader",
            ],
            from: "parent",
            to: [
                "#flutter_jit_runner",
                "#two_flutter",
            ],
        },
        {
            protocol: [ "fuchsia.ui.test.input.TouchInputListener" ],
            from: "parent",
            to: "#two_flutter",
        },
    ],
    expose: [
        {
            protocol: [ "fuchsia.ui.app.ViewProvider" ],
            from: "#two_flutter",
            to: "parent",
        },
    ],
    environments: [
        {
            name: "two_flutter_env",
            extends: "realm",
            runners: [
                {
                    runner: "flutter_jit_runner",
                    from: "#flutter_jit_runner",
                },
            ],
        },
    ],
}
